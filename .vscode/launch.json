  "version": "0.2.0",
  // "args": ["--path","C:/Users/ernes/Documents/VSK_model_explorer","-e"],
  // "args": ["--path","modules/many_bone_ik/demo","-e"],
  "configurations": [
    {
      "name": "Build the editor debug",
      "type": "cppvsdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": ["--path", "game", "-e"],
      "program": "${workspaceFolder}/godot/bin/godot.windows.editor.dev.x86_64.llvm.exe",
      "preLaunchTask": "Windows Debug"
    },
    {
      "name": "Build the editor release",
      "type": "cppvsdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": ["--path", "game", "-e"],
      "program": "${workspaceFolder}/godot/bin/godot.windows.editor.x86_64.llvm.exe",
      "preLaunchTask": "Windows Release"
    },
    {
      "name": "Build the editor release template",
      "type": "cppvsdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": ["--path", "game", "-e"],
      "program": "${workspaceFolder}/godot/bin/godot.windows.template_release.x86_64.llvm.console.exe",
      "preLaunchTask": "Windows Release Template"
    },
    {
      "name": "Build the editor debug template",
      "type": "cppvsdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": ["--path", "game", "-e"],
      "program": "${workspaceFolder}/godot/bin/godot.windows.template_debug.dev.x86_64.llvm.console.exe",
      "preLaunchTask": "Windows Debug Template"
    },
  ]
}
