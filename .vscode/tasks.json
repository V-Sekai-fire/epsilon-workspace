{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "options": {
    "env": {
      "CARGO_NET_GIT_FETCH_WITH_CLI": "true",
      // "SCONS_CACHE": "${workspaceFolder}/game/build/.cicd_cache"
    }
  },
  "tasks": [
    {
      "label": "Godot",
      "type": "shell",
      "command": "scons werror=no compiledb=yes precision=double optimze=speed_trace dev_build=no target=editor custom_modules=../modules/",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
