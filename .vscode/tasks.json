{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Windows",
      "type": "shell",
      "command": "scons target=editor bits=64 use_mingw=yes use_llvm=yes werror=yes warnings=all compiledb=yes warning=no test=yes debug_symbols=no platform=windows bits=64 use_thinlto=no warnings=no  LINKFLAGS='-Wl,-pdb=' CCFLAGS='-g -gcodeview' debug_symbols=no",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "docs",
      "path": "godot/platform/web",
      "problemMatcher": [],
      "label": "npm: docs - godot/platform/web",
      "detail": "jsdoc --template js/jsdoc2rst/ js/engine/engine.js js/engine/config.js js/engine/features.js --destination ''"
    }
  ]
}
