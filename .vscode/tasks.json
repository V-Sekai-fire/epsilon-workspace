{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dagger do build",
      "type": "shell",
      "command": "dagger-cue do build  --cache-to type=local,mode=max,dest=.dagger_storage --cache-from type=local,mode=max,src=.dagger_storage",
      "options": {
        "cwd": "${workspaceFolder}/game/.github/dagger"
      },
      "dependsOn": [
        "dagger project init",
        "dagger project update"
      ],
      "problemMatcher": [],
    },
    {
      "label": "dagger project init",
      "type": "shell",
      "command": "dagger-cue project init",
      "options": {
        "cwd": "${workspaceFolder}/game/.github/dagger"
      },
      "problemMatcher": [],
      "group": "build"
    },
    {
      "label": "dagger project update",
      "type": "shell",
      "command": "dagger-cue project update",
      "options": {
        "cwd": "${workspaceFolder}/game/.github/dagger"
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Windows Debug",
      "type": "shell",
      "command": "scons target=editor bits=64 use_mingw=yes use_llvm=yes werror=yes warnings=all dev_build=yes compiledb=yes warning=no test=yes debug_symbols=yes platform=windows bits=64 use_thinlto=no warnings=no  LINKFLAGS='-Wl,-pdb=' CCFLAGS='-g -gcodeview' debug_symbols=no custom_modules=../modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Windows Debug Template",
      "type": "shell",
      "command": "scons target=template_debug bits=64 use_mingw=yes use_llvm=yes werror=yes warnings=all dev_build=yes compiledb=yes warning=no test=yes debug_symbols=yes platform=windows bits=64 use_thinlto=no warnings=no  LINKFLAGS='-Wl,-pdb=' CCFLAGS='-g -gcodeview' debug_symbols=no custom_modules=../modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Windows Release",
      "type": "shell",
      "command": "scons target=editor bits=64 use_mingw=yes use_llvm=yes werror=yes warnings=all dev_build=no compiledb=yes warning=no test=yes debug_symbols=yes platform=windows bits=64 use_thinlto=no warnings=no  LINKFLAGS='-Wl,-pdb=' CCFLAGS='-g -gcodeview' debug_symbols=no custom_modules=../modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Windows Release Template",
      "type": "shell",
      "command": "scons target=template_release bits=64 use_mingw=yes use_llvm=yes werror=yes warnings=all dev_build=no compiledb=yes warning=no test=yes debug_symbols=yes platform=windows bits=64 use_thinlto=no warnings=no  LINKFLAGS='-Wl,-pdb=' CCFLAGS='-g -gcodeview' debug_symbols=no custom_modules=../modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Javascript Release Template",
      "type": "shell",
      "command": "scons target=template_release bits=64 use_mingw=yes use_llvm=yes werror=yes warnings=all dev_build=no compiledb=yes warning=no test=yes debug_symbols=yes platform=web bits=64 use_thinlto=no warnings=no debug_symbols=no custom_modules=../modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}
