{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "options": {
    "env": {
      // "SCONS_CACHE": "${workspaceFolder}/game/build/.cicd_cache"
    }
  },
  "tasks": [
    {
      "label": "Windows Release",
      "type": "shell",
      "command": "scons werror=yes platform=windows module_mvsqlite_enabled=yes use_module_mvsqlite=on target=editor use_lto=no deprecated=no optimize=speed use_mingw=yes use_llvm=yes use_thinlto=no warnings=no LINKFLAGS='-Wl,-pdb=' CCFLAGS='-Wall -Wno-tautological-compare -g -gcodeview' debug_symbols=no custom_modules=../modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
