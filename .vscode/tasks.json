{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "options": {
    "env": {
    }
  },
  "tasks": [
    {
      "label": "Godot Windows",
      "type": "shell",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "command": [
        "scons verbose=no vsproj=yes warnings=extra werror=yes compiledb=yes precision=double dev_build=yes optimize=speed_trace use_static_cpp=false target=editor custom_modules=../modules/"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
